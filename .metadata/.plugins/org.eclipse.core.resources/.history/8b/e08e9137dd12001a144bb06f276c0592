package shop;

public class AppAuthor {

	
	public static void main(String[] args) {
		 
		Author author1 = new Author ("Ella", "ella@gmail.com", 'f');
		System.out.println (author1);
		
		Book book1 = new Book ("Sunrise" , 100, 200, "Marc", "marc@gmail.com", 'm');
		System.out.println (book1);
		
	}

}
