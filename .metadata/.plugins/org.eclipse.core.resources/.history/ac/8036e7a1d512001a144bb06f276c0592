package com.accenture.lecture17;

public class Cylinder extends Circle {

	private double heigth;

	Cylinder() {
	}

	Cylinder(double heigth) {
		this.heigth = heigth;
	}

	Cylinder(double radius, double heigth; String color) {
		this.radius = radius;
		this.heigth = heigth;
		this.color = color;

	}
	
	Cylinder(double radius, double heigth; String color) {
		this.radius = radius;
		this.heigth = heigth;
		this.color = color;

	}
}
