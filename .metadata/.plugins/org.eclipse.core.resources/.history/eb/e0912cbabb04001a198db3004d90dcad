class Book {

	int pages;
	String autor;
	String title;

	public Book(){
		this(title, autor, pages);
	}

	Book book = new Book ("Cinderella", "No idea", 10);
	
	public Book(String title, String autor, int pages) {
		this.title = title;
		this.autor = autor;
		this.pages = pages;

		System.out.println("This book' s name is " + title + "It ' s autor is "
				+ autor + "It contents " + pages + "pages");
	}

	public String getTitle() {
		return title;

	}

	public String getAutor() {
		return autor;

	}

	public int getPages() {
		return pages;
	}

	public void setAge(int age) {
		this.age = age;
	}

}
