package com.acceture.lecture16;

public class WarriorApp {

 
	public static void main(String[] args) {
		 
		Warrior w1=new Warrior("Bobby", 5);
		w1.recievedDamage(3);
		System.out.println(w1.toString());
		w1.restoreHealth (98);
		System.out.println(w1);
		w1.restoreHealth (2);
		System.out.println(w1);
			
		
	}

}
